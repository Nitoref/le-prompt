{
    "args" : {
        "segments" : {
            "left": [
                "host",
                "pwd",
                "root",
                "exit"
            ],
            "right": [
                "jobs",
                "git",
                "time"
            ]
        },
        "time_format" : "%H:%M",
        "git_mode" : "simple",
        "cwd_mode" : "simple",
        "cwd_max_depth" : 3,
        "max_width_ratio"   : 0,
        "max_segment_width" : 0,
        "colorize_hostname" : false,
        "numeric_exit_codes" : true,
        "shorten_g_k_e_names" : false,
        "duration" : "",
        "path_aliases" : {
            "Desktop" : "Desktaupe",
            "Movies" : "Cheese"
        }
    },
    "symbols" : {
        "lock" :           "\uE0A2",
        "network" :        "\uE0A2",
        "separator" :      "\uE0B0",
        "separator_thin" : "\uE0B1",
        "git_branch" :     "\uE0A0",
        "git_detached" :   "\u27A6",
        "git_ahead" :      "\u2B06",
        "git_behind" :     "\u2B07",
        "git_staged" :     "\u2714",
        "git_not_staged" : "\u270E",
        "git_conflicted" : "\u273C",
        "git_untracked" :  "\u2026"
    },
    "theme" : {
        "separator":      { "fg":  15 },
        "username":       { "fg":  15, "bg" :   4},
        "username_root":  { "fg":  15, "bg" :   1},
        "hostname":       { "fg":  15, "bg" :   0},
        "home":           { "fg":  15, "bg" :   4},
        "path":           { "fg":  15, "bg" :  10},
        "cwd":            { "fg":  15, "bg" :  10},
        "readonly":       { "fg":   8, "bg" :   1},
        "ssh":            { "fg":   8, "bg" :   9},
        "docker_machine": { "fg":  13, "bg" :  55},
        "kube_cluster":   { "fg": -10, "bg" : -10},
        "kube_namespace": { "fg": -10, "bg" : -10},
        "dot_env":        { "fg":  15, "bg" :  55},
        "aws":            { "fg": -10, "bg" : -10},
        "repo_clean":     { "fg":  15, "bg" :   3},
        "repo_dirty":     { "fg":  15, "bg" :   5},
        "jobs":           { "fg":   4, "bg" :   0},
        "cmd_passed":     { "fg":  15, "bg" :   0},
        "cmd_failed":     { "fg":  15, "bg" :   5},
        "svn_changes":    { "fg":   2, "bg" :   3},
        "git_ahead":      { "fg":  14, "bg" :  10},
        "git_behind":     { "fg":  14, "bg" :  10},
        "git_staged":     { "fg":  15, "bg" :   2},
        "git_not_staged": { "fg":  15, "bg" :   9},
        "git_untracked":  { "fg":  15, "bg" :   1},
        "git_conflicted": { "fg":  15, "bg" :   1},
        "git_stashed":    { "fg":  15, "bg" :   4},
        "virtual_env":    { "fg":   8, "bg" :   6},
        "virtual_go":     { "fg":   8, "bg" :   6},
        "perlbrew":       { "fg":   8, "bg" :   4},
        "t_f_ws":         { "fg": -10, "bg" : -10},
        "time":           { "fg":  15, "bg" :   0},
        "shell_var":      { "fg":   1, "bg" :  11},
        "node":           { "fg":  15, "bg" :  40},
        "load":           { "fg":  15, "bg" :   2},
        "nix_shell":      { "fg":   0, "bg" :   4},
        "duration":       { "fg": -10, "bg" : -10}
    }
}
